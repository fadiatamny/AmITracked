{
    "manifest_version": 2,
    "name": "Test Extension",
    "version": "0.1",
    "browser_action": {
        "default_title": "Test Extension",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "trackerSnatcher.js"
            ]
        }
    ],
    "permissions": [
        "activeTab",
        "storage"
    ]
}